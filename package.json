{
  "name": "vrcx-next",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "chcp 65001 && electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "typeorm": "typeorm-ts-node-commonjs -d src/main/modules/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:generate -d src/main/modules/database/data-source.ts src/main/modules/database/migrations",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@fontsource-variable/geist": "^5.2.6",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "got": "^14.4.7",
    "hono": "^4.8.5",
    "hpagent": "^1.2.0",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.539.0",
    "mobx": "^6.13.7",
    "motion-v": "^1.7.0",
    "nanobus": "^4.5.0",
    "ogl": "^1.0.11",
    "reflect-metadata": "^0.2.2",
    "reka-ui": "^2.5.0",
    "set-cookie-parser": "^2.7.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socks-proxy-agent": "^8.0.5",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.27.10",
    "tail": "^2.2.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "three": "^0.180.0",
    "tw-animate-css": "^1.3.6",
    "typeorm": "^0.3.25",
    "uuid": "^13.0.0",
    "vee-validate": "^4.15.1",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.2",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "ws": "^8.18.3",
    "xstate": "^5.20.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/typed-ipc": "^1.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@swc/core": "^1.10.15",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.16.5",
    "@types/set-cookie-parser": "^2.4.10",
    "@types/tail": "^2.2.3",
    "@types/three": "^0.180.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.11",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "node-abi": "^4.12.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.17",
    "vue-component-type-helpers": "^3.0.5",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.3"
  }
}
